        {"TAG","03FF5C0A2BFA3A9B", "2022-02-01", "14:34:13"},
        {"TAG","03FF5C0A2BFA3A9B", "2022-02-01", "14:34:13"},
        {"BEACON","03FF5C0A2BFA3A9B", "2022-02-01", "14:34:13"}
    };
    string[] testConfig ={"BEACON", "TAG"};
    int configAmount = testConfig.Length;
    int lengthIndex = testData.GetLength(0);
    int i = 0;
    int j = 0;
}


<div class="mainContainer">
    <div class="infoContainer">
        <div class="systemLogs">
            <h1>Hola amigos!</h1>
        </div>
        <div class="systemComponentList">
            <p>@Model.message</p>
            @while (j < configAmount)
            {
                i = 0;
                @while (i < lengthIndex)
                {
                    @if (string.Equals(testData[i, 0], testConfig[j]))
                    {
                        <div class="systemComponent">
                            <p> Typ: @testData[i, 0] ID: @testData[i, 1]</p>
                            <p> Datum: @testData[i, 2] Tid: @testData[i, 3]</p>
                        </div>
                    }
                    i++;
                }
                j++;
            }
        </div>
    </div>
    <div class="navContainer">
        <div class="navButtonGroup">
            <a asp-area="" asp-page="/Index">
                <button class="buttonCurrent">
                    <span class="material-icons bigSize">home</span>
                    <br>Hem
                </button>
            </a>
            <a asp-area="" asp-page="/System">
                <button>
                    <span class="material-icons bigSize">dvr</span>
                    <br>System
                </button>
            </a>
            <a asp-area="" asp-page="/Settings">
                <button>
                    <span class="material-icons bigSize">settings</span>
                    <br>Inställningar
                </button>
            </a>
            <a asp-area="" asp-page="/Index">
                <button>
                    <span class="material-icons bigSize">logout</span>
                    <br>Logga ut
                </button>
            </a>
        </div>
    </div>
</div>
