        {"BEACON","03FF5C0A2BFA3A9B", "2022-02-01", "14:34:13"},
        {"BTAG","03FF5C0A2BFA3A9B", "2022-02-01", "14:34:13"},
        {"TAG","03FF5C0A2BFA3A9B", "2022-02-01", "14:34:13"},
